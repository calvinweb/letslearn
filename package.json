{
  "name": "letslearn",
  "version": "1.0.0",
  "description": "",
  "main": "build/electron.js",
  "scripts": {
    "clean": "rm -rf build/*",
    "startdev": "rollup -c rollup.config.js  --environment dev -w --sourcemap",
    "build_web": "rollup -c rollup.config.js --sourcemap",
    "build_electron": "rollup -c rollup.config.js --sourcemap && cp src/electron.js build/electron.js && cp package.electron.json build/package.json && electron-packager build/ --overwrite --ignore=node_modules --ignore=README.md --ignore=rollup.config.js --ignore=src",
    "run_electron":"rollup -c rollup.config.js --sourcemap   &&cp src/electron.js build/electron.js&&electron build/electron.js"
  },
  "author": "",
  "license": "GPL3",
  "dependencies": {
    "@mdi/js": "^7.0.96",
    "dompurify": "^2.4.0",
    "electron": "^21.0.1",
    "highlight.js": "^11.6.0",
    "lit": "^2.3.1",
    "localforage": "^1.10.0",
    "marked": "^4.1.1",
    "redux": "^4.2.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-html": "^0.2.1",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-resolve": "^0.0.1-predev.1",
    "rollup-plugin-serve": "^2.0.1",
    "showdown": "^2.1.0",
    "universalify": "^2.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-node-resolve": "^14.1.0",
    "@web/rollup-plugin-copy": "^0.3.0",
    "@web/rollup-plugin-html": "^1.11.0",
    "electron-packager": "^16.0.0",
    "rollup": "^2.79.1",
    "rollup-plugin-minify-html-literals": "^1.2.6",
    "rollup-plugin-summary": "^1.4.3",
    "rollup-plugin-terser": "^7.0.2"
  },
  "electronPackagerConfig": {
    "dir": "./build"
  }
}
